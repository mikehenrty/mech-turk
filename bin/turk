#!/usr/bin/env node
require('../config.js')(config => {
  'use strict';
  let Mechturk = require('../server/lib/mechturk.js');
  let mt = new Mechturk(config.PROD);
  let command = process.argv[2];
  mt.runCommand(command, process.argv[3])
    .catch(e => {
      console.error('Turk Error:', e);
    });
});
